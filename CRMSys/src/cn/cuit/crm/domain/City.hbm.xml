<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.cuit.crm.domain.City" table="b_city">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="identity" />
		</id>
		<property name="name" type="string">
			<column name="name" length="100" />
		</property>
		<property name="pycode" type="string">
			<column name="pycode" length="50" />
		</property>
		<property name="pid" type="integer">
			<column name="pid" />
		</property>
		<property name="postcode" type="string">
			<column name="postcode" length="50" />
		</property>
		<property name="areacode" type="string">
			<column name="areacode" length="50" />
		</property>
	</class>
</hibernate-mapping>
